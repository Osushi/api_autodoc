{
    "name": "osushi/apidoc",
    "description": "API markdown document generator",
    "keywords": ["api", "markdown", "md", "document", "doc", "docs", "generator"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/Osushi/apidoc",
