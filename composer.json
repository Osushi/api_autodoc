        }
    ],
    "require": {
        "php" : "~7.0.0 | ~7.1.0 | ~7.2.0",
        "twig/twig": "~2.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.13",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
